<tr class="raw-image-file-row" data-id="<%= @uploadItem.id %>">
  <td class="select">
  </td>
  <td class="name">
    <img src="/images/photo-preview.png" class="img-polaroid preview-image">
    <ul class="unstyled">
      <li><%= @uploadItem.file.name %></li>
      <li>
        <span class="label label-important">RAW image</span>
      </li>
    </ul>
  </td>
  <td class="actions">
    <button class="btn btn-danger delete-upload-item">Delete</button>
  </td>
</tr>
